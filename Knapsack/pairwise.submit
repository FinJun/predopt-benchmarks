############

getenv = True
request_cpus = 10
request_memory = 4 GB

Requirements      = TARGET.vm_name == "ITS-U20-NFS-20210413"

Universe       = vanilla
Executable     = ./pairwise.sh
Arguments      = $(modelname) $(loss) $(capacity) $(margin) $(lr) $(tag) $(id)

input   = /dev/null
#output  = ./out/img12run_$(id).out
error    = ./err/Knapsackpairwiserun_$(id).error
log       = ./log/Knapsackpairwiserun_$(id).condor_log

Queue modelname,loss,capacity,margin,lr,tag,id from pairwise.in