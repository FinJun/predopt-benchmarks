	############
#
# Example submit file for vanilla job
#
############

getenv = True
request_cpus = 18
request_memory = 16 GB

Requirements      = TARGET.vm_name == "ITS-U20-NFS-20210413"

Universe       = vanilla
Executable     = ./batch_run.sh
Arguments      = $(modelname) $(imgsz) $(lr) $(tag) $(id)

input   = /dev/null
#output  = ./out/img12run_$(id).out
error    = ./err/img12run_$(id).error
log       = ./log/img12run_$(id).condor_log

Queue modelname,imgsz,lr,tag,id from batch_run.in